{



	"set margin from list": {
		"prefix": "nm",
		"body": [
		  "nth(\\$m,)"
		],
		"description": "get margin list"
	},

	"set margin bottom from list": {
		"prefix": "nmb",
		"body": [
		  "margin-bottom: nth(\\$m,);"
		],
		"description": "set margin bottom from list"
	},
	


	"set padding from list": {
		"prefix": "np",
		"body": [
		  "nth(\\$p,)"
		],
		"description": "set padding from list"
	},

	"set padding tb0 lr from list": {
		"prefix": "nplr",
		"body": [
		  "padding: 0 nth(\\$p,);"
		],
		"description": "set padding tb0 lr from list"
	},

	"set padding tb from list": {
		"prefix": "nptb",
		"body": [
		  "padding: nth(\\$p,) 0;"
		],
		"description": "set padding tb from list"
	},

